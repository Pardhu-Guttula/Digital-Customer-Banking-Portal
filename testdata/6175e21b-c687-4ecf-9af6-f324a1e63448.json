{
  "test_verify_token_valid": {
    "tokens": [
      "valid_token",
      "token12345",
      "token_valid_2023",
      "auth_token_ABCxyz",
      "secure_token_7890"
    ],
    "expected": true
  },
  "test_verify_token_invalid": {
    "tokens": [
      "invalid_token",
      "token_invalid_54321",
      "wrong_token_XYZ",
      "unknown_token_999",
      "auth_invalid_token_123"
    ],
    "expected": false
  },
  "test_verify_token_empty": {
    "tokens": [
      "",
      "   ",
      "\t",
      "\n",
      null
    ],
    "expected": false
  },
  "test_verify_token_sql_injection": {
    "tokens": [
      "' OR '1'='1",
      "token' OR '1'='1",
      "' OR '1'='1' --",
      "' OR '1'='1' /*",
      "' OR '1'='1' /* SQL Injection Test */"
    ],
    "expected": false
  },
  "test_verify_token_db_error": {
    "tokens": [
      "token_database_error",
      "token_query_exception",
      "token_Dberror_0987",
      "error_token_xyz",
      "db_error_sim_123"
    ],
    "error_message": "Database error"
  }
}